				<TextField fx:id="numeroChamado" alignment="CENTER" promptText="NNNN" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</TextField>
				<TextField fx:id="tecnico" alignment="CENTER" promptText="Nome Tecnico" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />

			<children>
      <Rectangle arcHeight="20.0" arcWidth="20.0" fill="#22548000" height="511.0" layoutX="347.0" layoutY="113.0" smooth="false" stroke="#0000001c" strokeLineCap="ROUND" strokeType="INSIDE" width="664.0" />
      <VBox fx:id="textFilds" layoutX="618.0" layoutY="195.0" prefHeight="297.0" prefWidth="256.0" spacing="15.0">

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" blendMode="EXCLUSION" cache="true" cacheHint="SCALE" xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="agendamento.gui.GuiHome">
<AnchorPane prefHeight="720.0" prefWidth="1280.0">
			<children>
      <HBox prefHeight="100.0" prefWidth="200.0" />
				<VBox alignment="CENTER" layoutX="442.0" layoutY="135.0">
					<children>
						<Button fx:id="cadastrarVisitaBotao" mnemonicParsing="false" onAction="#chamarTelacadastrarVisita" prefHeight="100.0" prefWidth="350.0" text="Cadastrar Visita">
							<font>
								<Font name="Comic Sans MS" size="29.0" />
							</font>
						</Button>
						<Button mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" visible="false" />
						<Button fx:id="consultarVisitaBotao" mnemonicParsing="false" onAction="#chamarTelaConsultaVisita" prefHeight="100.0" prefWidth="350.0" text="Consultar Visita">
							<font>
								<Font name="Comic Sans MS" size="29.0" />
							</font>
						</Button>
						<Button mnemonicParsing="false" prefHeight="100.0" prefWidth="350.0" visible="false" />
						<Button fx:id="deletarVisitaBotao" mnemonicParsing="false" onAction="#chamarTelaDeletarVisita" prefHeight="100.0" prefWidth="350.0" text="Deletar Visita">
							<font>
								<Font name="Comic Sans MS" size="29.0" />
							</font>
						</Button>
					</children>
				</VBox>
			</children>
		</AnchorPane>
</HBox>

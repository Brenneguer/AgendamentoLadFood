<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.control.PasswordField?>

<HBox alignment="CENTER" blendMode="MULTIPLY" cache="true" cacheHint="SCALE" depthTest="ENABLE" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="agendamento.gui.GuiControllerCadastroUsuario">
		<AnchorPane blendMode="MULTIPLY" maxHeight="720.0" maxWidth="1280.0" minHeight="0.0" minWidth="0.0" prefHeight="720.0" prefWidth="1280.0">
			<children>
      <Rectangle arcHeight="20.0" arcWidth="20.0" fill="#22548000" height="511.0" layoutX="300.0" layoutY="73.0" smooth="false" stroke="#0000001c" strokeLineCap="ROUND" strokeType="INSIDE" width="664.0" />
      <VBox fx:id="textFilds" layoutX="531.0" layoutY="158.0" prefHeight="249.0" prefWidth="256.0" spacing="15.0">
         <children>
				<TextField fx:id="nome" alignment="CENTER" promptText="nome" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</TextField>
				<TextField fx:id="sobrenome" alignment="CENTER" promptText="sobrenome" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</TextField>
				<TextField fx:id="cpf" alignment="CENTER" promptText="xxx.xxx.xxx-xx" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</TextField>
				<TextField fx:id="mail" alignment="TOP_CENTER" promptText="email@email.com" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</TextField>
				<PasswordField fx:id="senha" alignment="TOP_CENTER" promptText="senha" scaleShape="false">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</PasswordField>
         </children>
      </VBox>
      <VBox fx:id="labels" layoutX="408.0" layoutY="164.0" prefHeight="290.0" prefWidth="110.0" spacing="28.0">
         <children>
				<Label scaleShape="false" text="Nome:" textAlignment="CENTER" wrapText="true">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Label>
				<Label scaleShape="false" text="Sobrenome:" textAlignment="CENTER" wrapText="true">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Label>
				<Label scaleShape="false" text="Cpf" textAlignment="CENTER" wrapText="true">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Label>
				<Label scaleShape="false" text="E-mail:" textAlignment="CENTER" wrapText="true">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Label>
				<Label scaleShape="false" text="Senha:" textAlignment="CENTER" wrapText="true">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Label>
         </children>
      </VBox>
				<Button fx:id="cancel" depthTest="ENABLE" layoutX="356.0" layoutY="530.0" mnemonicParsing="false" onAction="#cancelarButton" prefHeight="32.0" prefWidth="96.0" text="Cancelar">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Button>
				<Button fx:id="save" depthTest="ENABLE" layoutX="841.0" layoutY="532.0" mnemonicParsing="false" onAction="#salvarButton" prefHeight="32.0" prefWidth="96.0" text="Salvar">
					<font>
						<Font name="Comic Sans MS" size="18.0" />
					</font>
				</Button>
      <Button fx:id="inicio" blendMode="DARKEN" depthTest="ENABLE" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#selectInicio" prefHeight="31.0" prefWidth="175.0" text="Inicio">
         <font>
            <Font name="Comic Sans MS" size="18.0" />
         </font>
      </Button>
      <Pane fx:id="notice" layoutX="436.0" layoutY="305.0" prefHeight="79.0" prefWidth="406.0" visible="false">
         <children>
            <Label alignment="CENTER" prefHeight="25.0" prefWidth="405.0" text="Algo de inesperado ocorreu, tente novamente.">
               <font>
                  <Font name="Comic Sans MS" size="18.0" />
               </font>
            </Label>
            <Button fx:id="noticeBack" layoutX="177.0" layoutY="40.0" mnemonicParsing="false" onAction="#selectNoticeBack" text="Voltar">
               <font>
                  <Font name="Comic Sans MS" size="13.0" />
               </font>
            </Button>
         </children>
      </Pane>
			</children>
		</AnchorPane>

</HBox>
